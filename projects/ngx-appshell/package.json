    "@angular/core": ">=19.0.0",
    "@angular/material": ">=19.0.0",
    "@angular/animations": ">=19.0.0"
  },
  "dependencies": {
    "@angular/material": "19.2.16",
    "@azure/msal-angular": "3.1.0",
    "@azure/msal-browser": "3.27.0",
    "@nats-io/nats-core": "3.0.0",
    "@nats-io/jetstream": "3.0.0",
    "@nats-io/kv": "3.0.0",
    "marked": "15.0.11",
    "ngx-markdown": "19.1.1",
    "tslib": "2.3.0"
  },
  "sideEffects": false
}
